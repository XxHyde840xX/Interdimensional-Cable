      <li>MegascreenTelevision</li>
      <li>FlatscreenTelevision</li>
      <li>TubeTelevision</li>
	  <li MayRequire="VanillaExpanded.VFESpacer">UltrascreenTV</li>
    </televisionDefs>
    
    <!-- Time between each image in your show -->
    <secondsBetweenFrames>1.0</secondsBetweenFrames>

